    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "autoprefixer": "^10.4.14",
    "eslint-config-next": "^13.3.4",
    "eslint-plugin-next": "^0.0.0",
    "postcss": "^8.4.23",
